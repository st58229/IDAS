<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="267.0" prefWidth="285.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.RegisterWindowController">
   <children>
      <BorderPane layoutX="28.0" layoutY="25.0" prefHeight="219.0" prefWidth="229.0">
         <top>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="26.0" prefWidth="150.0" text="Nejste registrovaní?" textAlignment="CENTER" BorderPane.alignment="CENTER" />
         </top>
         <bottom>
            <Button fx:id="Register_Button" mnemonicParsing="false" onAction="#Register_Button_Clicked" prefHeight="26.0" prefWidth="150.0" text="Zaregistrujte se" BorderPane.alignment="CENTER" />
         </bottom>
         <center>
            <VBox alignment="TOP_CENTER" prefHeight="147.0" prefWidth="231.0" spacing="5.0" BorderPane.alignment="CENTER">
               <children>
                  <TextField fx:id="Register_mail" alignment="CENTER" prefHeight="26.0" prefWidth="150.0" promptText="Emailová adresa" />
                  <TextField fx:id="Register_name" alignment="CENTER" prefHeight="26.0" prefWidth="150.0" promptText="Křestní Jméno" />
                  <TextField fx:id="Register_surname" alignment="CENTER" prefHeight="26.0" prefWidth="150.0" promptText="Příjmení" />
                  <TextField fx:id="Register_userNameTextField" alignment="CENTER" prefHeight="26.0" prefWidth="150.0" promptText="Uživatelské jméno" />
                  <PasswordField fx:id="Register_Password" alignment="CENTER" promptText="Heslo" />
               </children>
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
